{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_TMDB_API_KEY": "@tmdb_api_key",
    "NEXT_PUBLIC_GA_MEASUREMENT_ID": "@ga_measurement_id"
  }
} 